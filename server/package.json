{
  "name": "nest-learn-series-backend",
  "version": "1.0.0",
  "description": "Backend for learn series",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "start:watch": "nodemon",
    "prestart:prod": "tsc",
    "start:prod": "node dist/main.js",
    "test": "jest",
    "test:watch": "jest --watchAll --coverage",
    "migration:film": "ts-node ./src/modules/film/migrations/main.ts"
  },
  "dependencies": {
    "@nestjs/common": "^4.5.9",
    "@nestjs/core": "^4.5.10",
    "@nestjs/microservices": "^4.5.8",
    "@nestjs/swagger": "^1.1.4",
    "@nestjs/testing": "^4.5.5",
    "dotenv": "^5.0.1",
    "mysql": "^2.15.0",
    "redis": "^2.7.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.6",
    "typeorm": "^0.1.16",
    "typescript": "^2.6.2"
  },
  "devDependencies": {
    "@types/jest": "^22.2.0",
    "@types/node": "^9.3.0",
    "jest": "^22.4.2",
    "nodemon": "^1.14.1",
    "ts-jest": "^22.4.1",
    "ts-node": "^4.1.0",
    "tslint": "5.3.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
